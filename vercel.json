{
  "version": 2,
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "styles/**",
      "use": "@vercel/static"
    },
    {
      "src": "scripts/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/about",
      "dest": "/about.html"
    },
    {
      "src": "/experience",
      "dest": "/experience.html"
    },
    {
      "src": "/skills",
      "dest": "/skills.html"
    },
    {
      "src": "/contact",
      "dest": "/contact.html"
    }
  ]
}